{
    "minimum-stability": "alpha",
    "name": "promet/drupal7-framework",
    "repositories": [
        {
            "type": "composer",
            "url": "http://static.drupal-packagist.org/v0.2.0"
        },
	{
            "type": "composer",
            "url": "http://static.drupal-packagist.org/v0.2.1"
        },
        {
            "type": "vcs",
            "url": "git@github.com:promet/drop_ship"
        },
        {
            "type": "vcs",
            "url": "git@github.com:promet/kw_manifests"
        }
    ],
    "require": {
        "ckeditor/ckeditor": "~4.4.4",
        "composer/composer": "~1.0@alpha",
        "drupal/drupal": "7.*",
        "drupal/tangler": "~0.1.3",
        "drupal/admin_menu": "7.*",
        "drupal/context": "7.*",
        "drupal/ctools": "7.*",
        "drupal/drop_ship": "1.*",
        "drupal/drupal-library-installer-plugin": "~0.1",
        "drupal/date": "7.*",
        "drupal/diff": "7.*",
        "drupal/entity": "7.*",
        "drupal/entityform": "7.*",
        "drupal/features": "7.2.*",
        "drupal/feeds": "7.*",
        "drupal/job_scheduler": "7.*",
        "drupal/kw_manifests": "1.*",
        "drupal/module_filter": "7.*",
        "drupal/omega": "7.4.*",
        "drupal/panels": "7.*",
        "drupal/registry_rebuild": "7.*",
        "drupal/rules": "7.*",
        "drupal/strongarm": "7.*",
        "drupal/views": "7.*",
        "drupal/wysiwyg": "7.*",
        "drush/drush": "6.*",
        "winmillwill/settings_compile": "~2.1.1"
    },
    "require-dev": {
        "drupal/devel": "7.*",
        "drupal/devel_themer": "7.1.*@dev",
        "drupal/diff": "~7.3.2",
        "drupal/drupal-extension": "0.1.5",
        "phpunit/phpunit": "*"
    },
    "extra": {
        "drupal-libraries": {
          "library-directory": "www/sites/all/libraries",
          "libraries": [
            {
              "name": "ckeditor",
              "package": "ckeditor/ckeditor"
            }
          ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "bin/wrapper",
            "vendor/bin/drupal_tangle"
        ],
        "post-update-cmd": [
            "vendor/bin/drupal_tangle"
        ]
    },
    "type": "project"
}
